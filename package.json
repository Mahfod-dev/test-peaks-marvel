{
  "name": "marvel-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "vite build",
    "preview": "vite preview --port 3000",
    "lint": "eslint --ext .js,.jsx,.html",
    "docs": "react-docgen --pretty ./src/components > docs.json",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@loadable/component": "^5.15.2",
    "@types/jest": "^29.2.5",
    "babel-jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "js-md5": "^0.7.3",
    "node-fetch": "^3.3.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-docgen-displayname-handler": "^3.0.2",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-router-dom": "^6.2.2"
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "babel-plugin-transform-import-meta": "^2.2.0",
    "eslint": "^8.32.0",
    "eslint-config-react-app": "^7.0.1",
    "husky": "^8.0.3",
    "jsdom": "^21.0.0",
    "lint-staged": "^13.1.0",
    "react-docgen": "^5.4.3",
    "vite": "^4.0.0",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.27.1"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --fix ",
    "*.{js,jsx,css,md,html,json}": "prettier --cache --write"
  }
